{
  "extends": ["airbnb", "prettier", "plugin:prettier/recommended"],
  "parser": "babel-eslint",
  "rules": {
    "implicit-arrow-linebreak": "off",
    "import/prefer-default-export": "off",
    "max-len": [
      "error",
      {
        "code": 130,
        "tabWidth": 2
      }
    ]
  },
  "env": {
    "browser": true
  }
}
